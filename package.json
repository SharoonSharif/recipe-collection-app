{
  "name": "recipe-vault",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun convex dev & bunx --bun vite",
    "build": "bunx --bun tsc && bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "start": "bunx --bun vite preview --port ${PORT:-4173} --host",
    "lint": "bunx --bun eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "convex:dev": "bunx --bun convex dev",
    "convex:deploy": "bunx --bun convex deploy"
  },
  "dependencies": {
    "@descope/react-sdk": "^1.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-router": "^1.20.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "convex": "^1.10.0",
    "lucide-react": "^0.320.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "^1.20.0",
    "@tanstack/router-vite-plugin": "^1.20.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}